datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model temprole {
    userId    String 
    roleId    String
    guildId   String
    expiresAt DateTime
    action TemproleMode
    @@id([userId, roleId, guildId, action])
}

enum TemproleMode {
  ADD
  REMOVE
}
